    <div>
        {{ form_label(form.health) }}
        {{ form_widget(form.health) }}
        <span class="error">{{ form_errors(form.health) }}</span>
    </div>

    <div>
        {{ form_label(form.quantitePlante) }}
        {{ form_widget(form.quantitePlante) }}
