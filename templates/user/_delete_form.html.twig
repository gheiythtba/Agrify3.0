<form method="post" action="{{ path('app_user_delete', {'user_id': user.userId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ user.userId) }}">
    <div class="btn-group">
        <button class="btnn" >Delete</button>
    </div>
</form>
