<form method="post" action="{{ path('app_field_delete', {'field_id': field.fieldId}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ field.fieldId) }}">
    <div class="btn-group">
        <button class="btnn" >Delete</button>
    </div>
</form>
